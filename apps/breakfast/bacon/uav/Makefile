COMPONENT=UAVSamplerC

BUFFER_SIZE?=16
CFLAGS+=-DBUFFER_SIZE=$(BUFFER_SIZE)
SAMPLE_INTERVAL?=650
CFLAGS+=-DSAMPLE_INTERVAL=$(SAMPLE_INTERVAL)

CFLAGS+=-DXT2_SMCLK
PFLAGS+=-DMSP430XV2_DCO_CONFIG=MSP430XV2_DCO_32MHz_RSEL6

CFLAGS+=-Isampler
CFLAGS+=-Isd
CFLAGS+=-I$(TOSDIR)/lib/serialprintf
CFLAGS+=-I$(TOSROOT)/apps/breakfast/util
PFLAGS+=-I$(TOSDIR)/chips/msp430/dma

include $(MAKERULES)
